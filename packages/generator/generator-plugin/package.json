{
  "name": "@modern-js/generator-plugin",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.0.1",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/plugin-i18n": "workspace:^1.0.0",
    "@modern-js/utils": "workspace:^1.0.0",
    "@modern-js/core": "workspace:^1.0.0",
    "@modern-js/generator-common": "workspace:^1.0.0",
    "@modern-js/generator-utils": "workspace:^1.0.0",
    "@modern-js/easy-form-core": "^1.0.0",
    "@modern-js/codesmith-api-npm": "^1.0.0",
    "@modern-js/codesmith-api-git": "^1.0.0",
    "@modern-js/codesmith-api-app": "^1.0.0",
    "@modern-js/codesmith": "^1.0.0",
    "uuid": "^8.3.2",
    "execa": "^5.0.0",
    "chalk": "^4.1.0",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.2",
    "handlebars": "^4.7.7",
    "isbinaryfile": "^4.0.6",
    "lodash": "^4.17.21",
    "memfs": "^3.2.0",
    "tmp": "^0.2.1",
    "unionfs": "^4.4.0",
    "vm2": "^3.9.2",
    "ora": "v5.4.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4",
    "@modern-js/module-tools": "^1.0.1",
    "@modern-js/plugin-testing": "^1.0.0",
    "@types/lodash": "^4.14.168",
    "@types/semver": "^7.3.8"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
